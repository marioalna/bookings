%tr
  %td{class: td_class.render}
    = custom_attribute.name
  %td{class: td_class.render}
    = custom_attribute.block_on_schedule ? t('admin.customAttributes.true1') : t('admin.customAttributes.false1')
  %td{class: td_class.render}
    .flex.gap-x-4.justify-end
      = render partial: 'shared/button_edit', locals:  {destination: edit_admin_custom_attribute_path(custom_attribute), data: { target: '_top' }}
      = render partial: 'shared/button_delete', locals:  {destination: {model: [:admin, custom_attribute]} }